{
  "AppSettings": {
    "Host": "https://memoar.art",
    "HttpRequestTimeoutMinutes": 10,
    "MaxBytesAllowedTraffic": 314572800,
    "Image": {
      "TargetSizeBytes": 5242880,
      "ImageQuality": 90,
      "ValidImageExtensions": [
        ".jpg",
        ".jpeg",
        ".png",
        ".gif"
      ]
    },
    "Video": {
      "ValidVideoExtensions": [
        ".mp4",
        ".mov",
        ".avi",
        ".mkv"
      ]
    },
    "Archive": {
      "MaxFileSize": 104857600,
      "ValidZipExtensions": [
        ".zip"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
